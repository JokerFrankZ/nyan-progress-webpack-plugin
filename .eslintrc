{
  "parser": "babel-eslint",
  "rules": {
    "no-trailing-spaces": [1],
    "space-before-blocks": [2, "always"],
    "no-unused-expressions": [0],
    "no-underscore-dangle": [0],
    "no-reserved-keys": [2],
    "no-multi-spaces": [0],
    "no-unused-vars": [1],
    "no-loop-func": [0],
    "new-cap": [0],
    "key-spacing": [0],
    "max-len": [1, 100],
    "strict": [0],
    "eol-last": [1],
    "no-console": [1],
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "curly": [0]
  },
  "settings": {
    "ecmascript": 6
  },
  "env": {
    "browser": true,
    "node": true
  }
}
